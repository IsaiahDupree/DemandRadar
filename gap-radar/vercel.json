{
  "framework": "nextjs",
  "buildCommand": "next build",
  "devCommand": "next dev --port ${PORT:-3945}",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "github": {
    "silent": true
  },
